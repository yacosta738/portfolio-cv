{
	"dependencies": {
		"@astrojs/check": "0.9.4",
		"@astrojs/partytown": "^2.1.4",
		"@astrojs/sitemap": "^3.5.1",
		"@tailwindcss/vite": "^4.1.17",
		"astro": "5.15.6",
		"astro-compress": "2.3.8",
		"hotkeypad": "1.0.2",
		"tailwindcss": "^4.1.17",
		"typescript": "5.9.3"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.3",
		"@iconify-json/lucide": "^1.2.73",
		"@iconify-json/mdi": "^1.2.3",
		"@iconify-json/simple-icons": "^1.2.58",
		"@iconify-json/tabler": "^1.2.23",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"astro-icon": "^1.1.5",
		"clsx": "^2.1.1",
		"happy-dom": "^20.0.10",
		"lefthook": "^2.0.4",
		"npm-run-all2": "^8.0.4",
		"rimraf": "^6.0.1",
		"semantic-release": "^24.2.7",
		"tailwind-merge": "^3.4.0",
		"vitest": "^4.0.9"
	},
	"name": "resume-portfolio",
	"scripts": {
		"astro": "astro",
		"build": "astro check && astro build",
		"check": "run-p check:*",
		"check:a11y": "biome lint --only=a11y ./src",
		"check:astro": "astro check",
		"check:biome": "biome check . --write",
		"clean": "rimraf dist",
		"dev": "npx @intellectronica/ruler apply && astro dev",
		"precommit": "npx @intellectronica/ruler apply",
		"preinstall": "npx only-allow pnpm",
		"prepare": "npx @intellectronica/ruler apply && pnpm exec lefthook install",
		"preview": "astro preview",
		"ruler:apply": "npx @intellectronica/ruler apply",
		"ruler:check": "npx @intellectronica/ruler check",
		"semantic-release": "semantic-release",
		"start": "astro dev",
		"test": "vitest",
		"update-deps": "pnpm update -i -r --latest"
	},
	"type": "module",
	"version": "0.0.1"
}
