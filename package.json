{
  "dependencies": {
    "@astrojs/check": "0.9.4",
    "@astrojs/sitemap": "^3.5.1",
    "@tailwindcss/vite": "^4.1.12",
    "astro": "5.13.5",
    "hotkeypad": "0.1.0",
    "tailwindcss": "^4.1.12",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@iconify-json/lucide": "^1.2.65",
    "@iconify-json/mdi": "^1.2.3",
    "@iconify-json/simple-icons": "^1.2.49",
    "@iconify-json/tabler": "^1.2.22",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "astro-icon": "^1.1.5",
    "clsx": "^2.1.1",
    "lefthook": "^1.12.3",
    "npm-run-all2": "^8.0.4",
    "rimraf": "^5.0.1",
    "semantic-release": "^24.2.7",
    "tailwind-merge": "^3.3.1",
    "vitest": "^3.2.4"
  },
  "name": "resume-portfolio",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "check": "run-p check:*",
    "check:a11y": "biome lint --only=a11y ./src",
    "check:astro": "astro check",
    "check:biome": "biome check . --write",
    "clean": "rimraf dist",
    "dev": "pnpm run ruler:apply && astro dev",
    "precommit": "pnpm run ruler:apply",
    "preinstall": "npx only-allow pnpm",
    "prepare": "pnpx @intellectronica/ruler apply && pnpm exec lefthook install",
    "preview": "astro preview",
    "ruler:apply": "ruler apply",
    "ruler:check": "ruler check",
    "semantic-release": "semantic-release",
    "start": "astro dev",
    "test": "vitest",
    "update-deps": "pnpm update -i -r --latest"
